    <fields>
        <fullName>Contact_Owner__c</fullName>
        <description>Primary owner of the contact or lead. Pulls the Contact_Owner__c field from the contact and sets all leads to &quot;Tech Transfer&quot;</description>
        <externalId>false</externalId>
        <formula>IF( NOT ( ISBLANK (ContactId) ),  Contact.Contact_Owner__c , &quot;Tech Transfer&quot;)</formula>
        <inlineHelpText>Primary owner of the contact or lead.</inlineHelpText>
        <label>Contact Owner</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
