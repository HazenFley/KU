    <fields>
        <fullName>CompanyAccount__c</fullName>
        <description>Fills in the company or Master Org for each Campaign Member</description>
        <externalId>false</externalId>
        <formula>BLANKVALUE(Contact.Account.Name,  Lead.Company )</formula>
        <label>Company/Account</label>
