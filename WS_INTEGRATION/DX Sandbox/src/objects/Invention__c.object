    <validationRules>
        <fullName>Closeout_Comments</fullName>
        <active>true</active>
        <description>Require Closeout Comments if invention is closed</description>
        <errorConditionFormula>AND(  Close_Invention__c = TRUE ,  ISBLANK(Closeout_Comments__c) )</errorConditionFormula>
        <errorMessage>Closeout Comments are required if you mark the invention as closed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closeout_Disposition</fullName>
        <active>true</active>
        <description>Require a Disposition when an invention is marked as closed</description>
        <errorConditionFormula>AND(  Close_Invention__c = TRUE ,  ISBLANK(TEXT(Disposition__c)) )</errorConditionFormula>
        <errorMessage>A disposition must be selected to close out an invention</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disclosure_before_Market_Ready</fullName>
        <active>true</active>
        <description>Require a disclosure date before invention marked as ready to market</description>
        <errorConditionFormula>AND(
ISBLANK( Disclosure_Execution_Date__c )
,
 Ready_for_Market__c = TRUE
)</errorConditionFormula>
        <errorMessage>The Disclosure Execution Date is required before the invention can be marked as Ready for Market.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EIR_Format</fullName>
        <active>true</active>
        <description>Format number field to match XXXXXXX-XX-XXXX (all numbers).</description>
        <errorConditionFormula>NOT(
OR(
ISBLANK( Invention_Report_Number_EIR__c )
,
REGEX( Invention_Report_Number_EIR__c , &quot;[0-9]{7}-[0-9]{2}-[0-9]{4}&quot;)
)
)</errorConditionFormula>
        <errorMessage>Please be sure to format the Invention Report Number (EIR) correctly. The format is XXXXXXX-XX-XXXX, all numbers.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EIR_Number</fullName>
        <active>true</active>
        <description>Require an EIR number when Initial Entry Date is populated</description>
        <errorConditionFormula>AND( 
NOT( ISBLANK(iEdison_Initial_Entry_Date__c) ) , 
ISBLANK( Invention_Report_Number_EIR__c ) 
)</errorConditionFormula>
        <errorMessage>If the invention has been submitted to iEdison, it should have an Invention Report Number (EIR).
Please enter that value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>External_Assessment_Required</fullName>
        <active>true</active>
        <description>When the prior art search is completed, the External Assessment Required field must be filled in. they should know at this point.</description>
        <errorConditionFormula>AND(
NOT(ISBLANK( Prior_Art_Search_Completed_Date__c ))
,
 ISPICKVAL( External_Assessment_Required__c , &quot;&quot;) 
)</errorConditionFormula>
        <errorMessage>Please designate whether an external assessment is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>External_Assessment_Score</fullName>
        <active>true</active>
        <description>Require a score if marked as required before the invention can be moved to market.</description>
        <errorConditionFormula>AND(
Ready_for_Market__c =TRUE
,
 ISPICKVAL(External_Assessment_Required__c , &quot;Yes&quot;) 
,
ISNULL( External_Assessment_Score__c )
)</errorConditionFormula>
        <errorMessage>Please be sure to record the External Assessment Score.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>External_Lead_Institution_Required</fullName>
        <active>true</active>
        <description>Require a lead institution be selected when the Lead Institution field is set to External.</description>
        <errorConditionFormula>AND (
ISPICKVAL( Lead_Institution__c , &quot;External&quot;) 
,
ISBLANK(External_Lead_Institution__c)
)</errorConditionFormula>
        <errorMessage>If the Lead Institution is external, please define that institution in the External Lead Institution field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Initial_iEdison_Entry_Rq_Disclosure_Date</fullName>
        <active>true</active>
        <description>Initial iEdison Entry requires a Disclosure Execution Date</description>
        <errorConditionFormula>AND(
ISBLANK( Disclosure_Execution_Date__c )
,
NOT(ISBLANK( iEdison_Initial_Entry_Date__c ))
)</errorConditionFormula>
        <errorMessage>An invention must first be disclosed to KU before it can be reported to iEdison.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Internal_Assessment_Score</fullName>
        <active>true</active>
        <description>Require Internal Assessment Score before Invention can be moved to market</description>
        <errorConditionFormula>AND(
ISBLANK( Internal_Assessment_Score__c )
,
 Ready_for_Market__c = TRUE
)</errorConditionFormula>
        <errorMessage>Please be sure to record the internal assessment score.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>On_Hold_Comments</fullName>
        <active>true</active>
        <description>Require &quot;On Hold Comments&quot; if &quot;On Hold&quot; is checked</description>
        <errorConditionFormula>AND( On_Hold__c ,  ISBLANK( On_Hold_Comments__c ) )</errorConditionFormula>
        <errorMessage>A comment is required in the field &quot;On Hold Comments&quot; since the invention is marked as &quot;On Hold&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>On_Hold_Disclosure_Date</fullName>
        <active>true</active>
        <description>On Hold requires a Disclosure Date</description>
        <errorConditionFormula>AND(
 On_Hold__c = TRUE
,
 ISBLANK(Disclosure_Execution_Date__c)
)</errorConditionFormula>
        <errorMessage>Inventions cannot be placed On Hold until they have a Disclosure Execution Date. Please either uncheck On Hold or fill in the Disclosure Execution Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prior_Art_Search_Before_Market</fullName>
        <active>true</active>
        <description>Require Prior Art Search Completed Date before Ready for Market can be checked.</description>
        <errorConditionFormula>AND(
ISBLANK( Prior_Art_Search_Completed_Date__c )
,
Ready_for_Market__c =TRUE
)</errorConditionFormula>
        <errorMessage>Please be sure to complete the Prior Art Search before this invention is moved to Market Status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prior_Art_Search_Comments</fullName>
        <active>true</active>
        <description>When Prior Art Search Completed Date is populated, require the accompanying comments.</description>
        <errorConditionFormula>AND(
NOT(ISBLANK( Prior_Art_Search_Completed_Date__c ))
,
ISBLANK( Prior_Art_Search_Comments__c )
)</errorConditionFormula>
        <errorMessage>Please be sure to leave some comments in the Prior Art Search Comment box.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Waive_Reason</fullName>
        <active>true</active>
        <description>Require Waive reason when Waiver Date populated</description>
        <errorConditionFormula>AND( 
NOT(ISBLANK( Waiver_Date__c )),
ISPICKVAL(Not_Elect_Title_Reason__c , &quot;&quot;)
)</errorConditionFormula>
        <errorMessage>Please choose a waive reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Waive_Reason_Other_Comment</fullName>
        <active>true</active>
        <description>If Waive Reason of Other is chosen, require a comment</description>
        <errorConditionFormula>AND(
ISPICKVAL( Not_Elect_Title_Reason__c , &quot;Other&quot;) 
,
ISBLANK( Not_Elect_Title_Other_Reason__c )
)</errorConditionFormula>
        <errorMessage>Not Elect Title Other Reason is blank.
Please give a reason in the comment field.</errorMessage>
    </validationRules>
</CustomObject>
