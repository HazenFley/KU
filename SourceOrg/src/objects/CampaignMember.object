                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Member_Owner_Check__c</fullName>
        <description>This will be checked if the owner of the contact or lead is the same as the owner of the campaign. This will help discover leads or contacts that are not assigned correctly, which is helpful for task assignment.</description>
        <externalId>false</externalId>
        <formula>OR( Contact.OwnerId = Campaign.OwnerId ,  Lead.OwnerId  = Campaign.OwnerId )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Member Owner Check</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Member_Owner__c</fullName>
        <description>Gives the owner name for the member</description>
        <externalId>false</externalId>
        <formula>BLANKVALUE(  Contact.Owner.FirstName &amp; &quot; &quot; &amp; Contact.Owner.LastName  , Lead.Owner:User.FirstName &amp; &quot; &quot; &amp; Lead.Owner:User.LastName  )</formula>
        <label>Member Owner</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
